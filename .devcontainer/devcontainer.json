{
    "name": "Python 3.10 with Jupyter and Data Science Libraries",
    "image": "mcr.microsoft.com/devcontainers/python:3.10",
    "features": {
      "ghcr.io/devcontainers/features/jupyter-python:1": {}
    },
    "postCreateCommand": "pip install notebook numpy matplotlib pandas scikit-learn",
    "forwardPorts": [8888],
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-toolsai.jupyter"
        ]
      }
    }
  }
  